{
  "scripts": {
    "clean": "rm -rf app/assets/*",
    "sync": "pos-cli sync dev",
    "watch": "npx webpack-cli -w",
    "lr": "npx livereload \"app\" -e \"liquid,graphql,js,css\"",
    "start": "npx concurrently npm:clean npm:watch npm:sync npm:lr",
    "build": "npm run clean && NODE_ENV=production npx webpack-cli",
    "build:dev": "npm run clean && NODE_ENV=development npx webpack-cli"
  },
  "devDependencies": {
