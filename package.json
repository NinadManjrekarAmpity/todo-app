    "@babel/preset-env": "^7.7.7",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "css-loader": "^3.4.1",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^7.0.26",
    "postcss-csso": "^4.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "tailwindcss": "^1.1.4",
